{
  "name": "ion-calendar",
  "version": "7.17.2",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "prepare": "npm run build && cd dist/ion-calendar && npm link && cd ../../projects/demo && npm link @heliomarpm/ion-calendar",
    "prebuild": "rimraf -rf dist",
    "build": "ng build ion-calendar",
    "build:demo": "ng build demo",
    "package": "npm run build && cd dist/ion-calendar && npm pack",
    "start": "ng serve -o",
    "start:demo": "ng serve demo",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "upversion": "node upversion.js",
    "deploy": "node ./node_modules/gh_deploy/index.mjs"
  },
  "private": false,
  "dependencies": {
    "@angular/animations": "^17.3.12",
